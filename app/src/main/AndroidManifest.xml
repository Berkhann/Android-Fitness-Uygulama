<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.atakan.fitnessgunlukleri">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="FitnessGünlükleri"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Programs" />
        <activity android:name=".ListsActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".ContentDesign" />
        <activity android:name=".MainActivity" />
        <activity android:name=".MyProgram" />
        <activity android:name=".ReadyProgram" />
        <activity android:name=".ReadyProgramList" />
        <activity android:name=".Selected_Prog"></activity>
        <activity android:name=".popUpMyprog"
            android:theme="@style/AppTheme.CustomTheme"
            >
        </activity>
        <activity android:name=".ContentDesignPopUp"
            android:theme="@style/AppTheme.CustomTheme"
            >
        </activity>
    </application>

</manifest>